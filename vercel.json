{
  "version": 2,
  "routes": [
    {
      "src": "/hello",
      "dest": "/hello.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/api/vote/(.*)",
      "dest": "/api/vote/[id].js"
    },
    {
      "src": "/api/votes",
      "dest": "/api/votes.js"
    }
  ]
}
